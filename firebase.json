{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/sendMessagesForEvent",
      "function": "sendMessagesForEvent"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }]
  }
}
